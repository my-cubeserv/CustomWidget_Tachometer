{
	"name": "Tachometer",
	"description": "Chart Tachometer",
	"eula": "",
	"vendor": "Marina Yesayan",
	"license": "",
	"id": "chart.tachometer",	
	"newInstancePrefix": "TachometerChart",
	"version": "1.0.0",	
	"icon": "data:image/png;base64,",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "chart-tachometer",
			"url": "https://my-cubeserv.github.io/CustomWidget_Tachometer/main-component.js",
			"integrity": "",
			"ignoreIntegrity": true
		},
		{
			"kind": "styling",
			"tag": "chart-style",
			"url": "https://my-cubeserv.github.io/CustomWidget_Tachometer/styling-component.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
	],
	"imports": [
		"datasource"
	],
	"properties": {
		
		"selection": {
			"type": "Selection"
		}
	},
	"methods": {
		"getSelection": {
			"description": "get the currently selected node",
			"returnType": "Selection",
			"parameters": [],
			"body": "return this.selection;"
		},
		"setvalue": {
			"description": "Set the value.",
			"parameters": [
				{
					"name": "newValue",
					"type": "number",
					"description": "Select top number"
				}
			],
			"body": "this.value = newValue;"
		},
		"setcolor": {
			"description": "Set color.",
			"parameters": [
				{
					"name": "newColor",
					"type": "string",
					"description": "Select new color"
				}
			],
			"body": "this.color = newColor;"
		},
		"setfontsize": {
			"description": "Set font size.",
			"parameters": [
				{
					"name": "newFontsize",
					"type": "number",
					"description": "Select font size"
				}
			],
			"body": "this.fontsize = newFontsize;"
		},
		"getcolor": {
			"returnType": "string",
			"description": "Returns selected color.",
			"body": "return this.color;"
		},
		"getfontsize": {
			"returnType": "number",
			"description": "Returns the selected font size.",
			"body": "return this.fontsize;"
		},
		"update": {
			"description": "Rerenders the chart",
			"parameters": []
		}
	},
	"events": {
		"onSelect": {
			"description": "This event is fired, when the user selects a node in the chart."
		}
	},
	"dataBindings": {
		"myDataSource": {
			"feeds": [
				{
					"id": "measures",
					"description": "The measures feed",
					"type": "mainStructureMember"
				}
			]
		}
	}
}
